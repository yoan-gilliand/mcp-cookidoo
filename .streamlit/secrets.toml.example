# Streamlit Secrets Configuration
# Copy this file to .streamlit/secrets.toml and fill in your values

# App password - users must enter this to access the app
app_password = "your-secure-password-here"

# Cookidoo credentials - used for all API operations
cookidoo_email = "your-email@example.com"
cookidoo_password = "your-cookidoo-password"

# Gemini API key - get one at https://aistudio.google.com/apikey
gemini_api_key = "your-gemini-api-key"
